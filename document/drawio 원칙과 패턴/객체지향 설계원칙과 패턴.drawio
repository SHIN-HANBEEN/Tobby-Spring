        <mxCell id="D_0XX4LfLNMpEoH9oSPd-11" parent="D_0XX4LfLNMpEoH9oSPd-16" style="rounded=0;whiteSpace=wrap;html=1;textShadow=1;sketch=1;curveFitting=1;jiggle=2;verticalAlign=middle;align=center;spacingTop=0;" value="DConnectionMaker" vertex="1">
